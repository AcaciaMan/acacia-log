{
  "name": "acacia-log",
  "displayName": "acacia-log",
  "description": "log files processing, similar lines, patterns",
  "repository": {
    "type": "git",
    "url": "https://github.com/AcaciaMan/acacia-log"
  },
  "version": "3.6.2",
  "publisher": "manacacia",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "log",
    "log viewer",
    "log analyzer",
    "log file",
    "log patterns",
    "log search",
    "log timeline",
    "similar lines",
    "log processing",
    "log navigation",
    "datetime search"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.setLogDateFormat",
        "title": "Log navigate to Date Format",
        "category": "Acacia Log",
        "description": "Sets the date format used in the log files."
      },
      {
        "command": "extension.setLogDateRegex",
        "title": "Log navigate to Date RegExp",
        "category": "Acacia Log",
        "description": "Sets the regular expression to match the date in the log files."
      },
      {
        "command": "extension.setLogSearchDate",
        "title": "Log navigate to Search Date",
        "category": "Acacia Log",
        "description": "Sets the date to search for in the log files (YYYY-MM-DD)."
      },
      {
        "command": "extension.setLogSearchTime",
        "title": "Log navigate to Search Time",
        "category": "Acacia Log",
        "description": "Sets the time to search for in the log file and Runs the search."
      },
      {
        "command": "extension.calculateSimilarLineCounts",
        "title": "Calculate Similar Line Counts",
        "category": "Acacia Log",
        "description": "Calculates counts of similar lines in the log file and displays them in descending order."
      },
      {
        "command": "extension.drawLogTimeline",
        "title": "Draw Log Timeline",
        "category": "Acacia Log",
        "description": "Draws a chart containing a timeline of log file records."
      },
      {
        "command": "acacia-log.logExplorer.refresh",
        "title": "Refresh Log Files",
        "category": "Acacia Log",
        "icon": "$(refresh)"
      },
      {
        "command": "acacia-log.logExplorer.addFolder",
        "title": "Add Log Folder",
        "category": "Acacia Log",
        "icon": "$(add)"
      },
      {
        "command": "acacia-log.logExplorer.removeFolder",
        "title": "Remove Folder",
        "category": "Acacia Log",
        "icon": "$(remove)"
      },
      {
        "command": "acacia-log.logExplorer.openFile",
        "title": "Open Log File",
        "category": "Acacia Log"
      },
      {
        "command": "acacia-log.logExplorer.showFileInfo",
        "title": "Show File Information",
        "category": "Acacia Log",
        "icon": "$(info)"
      },
      {
        "command": "acacia-log.logExplorer.revealInExplorer",
        "title": "Reveal in File Explorer",
        "category": "Acacia Log"
      },
      {
        "command": "acacia-log.logExplorer.generateGapReport",
        "title": "HTML Report",
        "category": "Acacia Log",
        "icon": "$(graph-scatter)"
      },
      {
        "command": "acacia-log.unifiedView.switchToLogAnalysis",
        "title": "Log Analysis",
        "category": "Acacia Log",
        "icon": "$(search)"
      },
      {
        "command": "acacia-log.unifiedView.switchToSimilarLines",
        "title": "Similar Lines",
        "category": "Acacia Log",
        "icon": "$(graph)"
      },
      {
        "command": "acacia-log.unifiedView.switchToTimeline",
        "title": "Timeline",
        "category": "Acacia Log",
        "icon": "$(graph-line)"
      },
      {
        "command": "acacia-log.unifiedView.switchToPatternSearch",
        "title": "Pattern Search",
        "category": "Acacia Log",
        "icon": "$(search-fuzzy)"
      },
      {
        "command": "acacia-log.unifiedView.switchToFileInfo",
        "title": "File Info",
        "category": "Acacia Log",
        "icon": "$(info)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "myExtensionContainer",
          "title": "Acacia Log",
          "icon": "resources/log_icon.svg"
        }
      ]
    },
    "views": {
      "myExtensionContainer": [
        {
          "id": "acacia-log.logExplorer",
          "name": "Log Files",
          "icon": "resources/log_icon.svg"
        },
        {
          "type": "webview",
          "id": "acacia-log.unifiedView",
          "name": "Log Analysis",
          "icon": "resources/log_icon.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "acacia-log.logExplorer.refresh",
          "when": "view == acacia-log.logExplorer",
          "group": "navigation"
        },
        {
          "command": "acacia-log.logExplorer.addFolder",
          "when": "view == acacia-log.logExplorer",
          "group": "navigation"
        },
        {
          "command": "acacia-log.unifiedView.switchToLogAnalysis",
          "when": "view == acacia-log.unifiedView",
          "group": "navigation@1"
        },
        {
          "command": "acacia-log.unifiedView.switchToSimilarLines",
          "when": "view == acacia-log.unifiedView",
          "group": "navigation@2"
        },
        {
          "command": "acacia-log.unifiedView.switchToTimeline",
          "when": "view == acacia-log.unifiedView",
          "group": "navigation@3"
        },
        {
          "command": "acacia-log.unifiedView.switchToPatternSearch",
          "when": "view == acacia-log.unifiedView",
          "group": "navigation@4"
        },
        {
          "command": "acacia-log.unifiedView.switchToFileInfo",
          "when": "view == acacia-log.unifiedView",
          "group": "navigation@5"
        },
        {
          "command": "acacia-log.logExplorer.generateGapReport",
          "when": "view == acacia-log.unifiedView",
          "group": "navigation@6"
        }
      ],
      "view/item/context": [
        {
          "command": "acacia-log.logExplorer.openFile",
          "when": "view == acacia-log.logExplorer && viewItem == logFile",
          "group": "navigation"
        },
        {
          "command": "acacia-log.logExplorer.showFileInfo",
          "when": "view == acacia-log.logExplorer && viewItem == logFile",
          "group": "navigation"
        },
        {
          "command": "acacia-log.logExplorer.revealInExplorer",
          "when": "view == acacia-log.logExplorer && viewItem == logFile",
          "group": "navigation"
        },
        {
          "command": "acacia-log.logExplorer.removeFolder",
          "when": "view == acacia-log.logExplorer && viewItem == logFolder",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Acacia Log Settings",
      "properties": {
        "acacia-log.logDateFormat": {
          "type": "string",
          "default": "yyyy-MM-dd HH:mm:ss",
          "description": "The date format used in the log files."
        },
        "acacia-log.logDateRegex": {
          "type": "string",
          "default": "\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}",
          "description": "The regular expression to match the date in the log files."
        },
        "acacia-log.logSearchDate": {
          "type": "string",
          "default": "2023-01-01",
          "description": "The date to search for in the log files (YYYY-MM-DD)."
        },
        "acacia-log.logSearchTime": {
          "type": "string",
          "default": "12:00:00",
          "description": "The time to search for in the log files (HH:mm:ss)."
        },
        "acacia-log.logFilePath": {
          "type": "string",
          "default": "",
          "description": "The path to the log file."
        },
        "acacia-log.patternsFilePath": {
          "type": "string",
          "default": "",
          "description": "The path to the search patterns JSON file."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "jest"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "luxon": "^3.5.0",
    "ts-jest": "^29.2.5"
  },
  "icon": "log_icon.png"
}
